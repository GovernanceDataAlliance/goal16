<div class="m-popup">
  <button class="btn-close">
    <svg class="icon icon-close">
      <use xlink:href="#icon-close"></use>
    </svg>
  </button>
  <div class="wrapper">

    <a href="countries/FRA"><h1 class="c-title-popup -subtitle">{{name}}</h1></a>
    <p class="c-subtitle-popup">OFFICIAL INDICATORS</p>
    {{#each indicators}}
    <div class="data-numbers">
      <span class="data-numbers-text">{{title}}</span>
      <span class="c-score-popup -small">{{score}}</span>
    </div>
    {{/each}}
    <div class="links">
      <a href="{{url}}/countries/?iso={{iso}}">visit country page > </a>
      <a href="{{url}}/compare/?isoA={{iso}}">compare to others > </a>

    </div>
  </div>
</div>
<div id="popup-background"></div>
