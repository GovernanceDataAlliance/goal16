{{#each targets}}
  <div class="m-table-target" id="{{slug}}">
    <div class="header">
      <h2>{{code}} {{title}}</h2>
    </div>
    <div class="m-table-indicators js--indicators">
      {{#each indicators}}
      <div class="m-table-indicator">
        <div class="header">
          <h1>{{title}} <span>{{type}}</span></h1>
        </div>
        <div class="indicator-info">
          <p>{{desciption}}</p>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
{{/each}}
